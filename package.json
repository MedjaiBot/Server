    "all-contributors-cli": "^6.6.1",
    "chokidar": "^3.0.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/winston": "^2.4.4",
    "dotenv": "^8.0.0",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "webpack-cli": "^3.3.2",
    "winston": "^3.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
